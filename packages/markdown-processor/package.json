{
  "name": "@saitamau-maximum/markdown-processor",
  "version": "0.0.0",
  "private": false,
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint --fix --ext .ts,.tsx src",
    "lint:check": "eslint --ext .ts,.tsx src",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "clean": "rimraf dist",
    "build": "npm run clean && tsup",
    "test": "vitest run"
  },
  "keywords": [],
  "author": "sor4chi",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "refractor": "^4.8.1",
    "rehype-katex": "^7.0.0",
    "rehype-mermaidjs": "^2.0.0",
    "rehype-parse": "^9.0.0",
    "rehype-prism-plus": "^2.0.0",
    "rehype-react": "^8.0.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.0",
    "remark-breaks": "^4.0.0",
    "remark-directive": "^3.0.0",
    "remark-extract-toc": "^1.1.0",
    "remark-flexible-code-titles": "^1.1.4",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.0",
    "remark-slug": "^7.0.1",
    "unified": "^11.0.4"
  },
  "devDependencies": {
    "@types/react": "^18.2.58",
    "glob": "^10.3.10",
    "rimraf": "^5.0.5"
  },
  "files": [
    "dist"
  ],
  "exports": {
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js"
    },
    "./react": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js"
    }
  },
  "typesVersions": {
    "*": {
      "server": [
        "./dist/server"
      ],
      "react": [
        "./dist/client"
      ]
    }
  }
}
